%%%%%%%%%%%%%%%%%%%%%%%%
% Korrelation awgn()
%%%%%%%%%%%%%%%%%%%%%%%%

delete(findall(0,'type','line'));

noise=wgn(1e4,1,1);

f1=figure(1);
SUB=220;


subplot(SUB+1);
plot(noise);
grid on;


lxx=xcorr(noise,noise);

subplot(SUB+2);
plot(lxx);
grid on;

noiseN=noise./max(noise));

subplot(SUB+3);
plot(noise);
grid on;


lxx=xcorr(noise,noise);

subplot(SUB+2);
plot(lxx);
grid on;





















