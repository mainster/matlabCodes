
fc=37;
fs=1600;

Tc=1/fc;
Ts=1/fs;

DCre=0;
DCim=0;
phi=0;
Are=1;
Aim=1;


% if 0==mod(fc,1)
%     n=fs/fc*fc*32;
%     disp('1 gewählt');
% else
%     n=fs/fc*10*fc*32;
%     disp('2 gewählt');
% end


k=1600*37;
%k=k/gcd(k,32);

t=[0:Ts:k*Ts-Ts]; 
sig=Aim*sin(2*pi*fc*t);
plot(t,sig);grid;

disp('')
mod(length(t),32)

length(t)

sig(1)
sig(end)